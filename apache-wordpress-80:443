<VirtualHost *:80>
        ServerName www.suhab.am.gov.br
        ServerAlias suhab.am.gov.br
        Redirect permanent / https://www.suhab.am.gov.br
</VirtualHost>

<VirtualHost *:443>
        ServerAdmin webmaster@localhost
        ServerName www.suhab.am.gov.br
        ServerAlias suhab.am.gov.br
        DocumentRoot /var/www/suhab

        SSLEngine On
        SSLCertificateFile              /etc/apache2/ssl/suhab/suhab.crt
        SSLCertificateKeyFile           /etc/apache2/ssl/suhab/private.key
        SSLCertificateChainFile         /etc/apache2/ssl/suhab/intermediate.crt

        <Directory />
                Options FollowSymLinks
                AllowOverride None
        </Directory>

        <Directory /var/www/suhab/>
                Options -Indexes FollowSymLinks MultiViews
                AllowOverride All
                Order allow,deny
                allow from all
                #allow from 200.242.37.25/32 200.174.153.42/32
        </Directory>

